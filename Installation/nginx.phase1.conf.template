server {
	listen EXPOSED_PORT_GOES_HERE;
	listen [::]:EXPOSED_PORT_GOES_HERE;

	server_name DOMAIN_NAME_GOES_HERE;

        location / {
                proxy_set_header Host $host;
                proxy_set_header X-Real-IP $remote_addr;
                proxy_pass http://127.0.0.1:APPLICATION_PORT_GOES_HERE;
        }
}
